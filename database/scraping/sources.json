  "scraping_rules": {
    "min_price_usd": 5000,
    "max_price_usd": 50000000,
    "required_fields": ["title", "price_text", "location"],
    "duplicate_detection": "title_similarity",
    "image_download": true,
    "max_images_per_property": 10,
    "respect_robots_txt": true,
    "user_agent": "CostaRica-MLS-Bot/1.0",
    "delay_between_requests": 1500,
    "max_retries": 3,
    "priority_order": [
      "priority_1_agents",
      "priority_2_agencies",
      "general_classifieds"
    ],
    "scrape_order": "priority_first",
    "max_properties_per_source": 500,
    "categories": {
      "luxury": ["luxury", "premium", "high-end", "exclusive"],
      "residential": ["house", "home", "villa", "residential"],
      "commercial": ["commercial", "office", "retail", "business"],
      "land": ["land", "lot", "terrain", "terreno"]
